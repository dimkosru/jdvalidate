{
    "name": "jedi-validate",
    "version": "1.5.1",
    "description": "A lightweight form validation component",
    "main": "./dist/jedi-validate.js",
    "scripts": {
        "prestart": "rimraf ./dist",
        "start": "webpack-dev-server --hot --inline --progress --colors --watch --port 4000",
        "prebuild": "npm run esdoc && rimraf ./dist",
        "build": "cross-env NODE_ENV=production webpack --progress --colors",
        "lint": "eslint src/**/*.js",
        "lint:fix": "eslint src/**/*.js --fix",
        "lint:fix-tests": "eslint tests/**/*.js --fix",
        "preesdoc": "rimraf ./docs",
        "esdoc": "esdoc -c esdoc.json",
        "test": "cross-env NODE_ENV=test karma start --single-run",
        "tests": "cross-env NODE_ENV=test karma start"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dmitriytat/jedi-validate.git"
    },
    "keywords": [
        "form",
        "validation",
        "formdata"
    ],
    "author": "Dmitriy Tatarintsev <tatarintsevd@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/dmitriytat/jedi-validate/issues"
    },
    "homepage": "https://github.com/dmitriytat/jedi-validate#readme",
    "devDependencies": {
        "@types/chai": "^4.1.3",
        "@types/mocha": "^5.2.0",
        "@types/should": "^13.0.0",
        "@types/sinon": "^4.3.3",
        "babel-core": "^6.26.3",
        "babel-eslint": "^8.2.3",
        "babel-loader": "^7.1.4",
        "babel-minify-webpack-plugin": "^0.3.1",
        "babel-plugin-istanbul": "^4.1.6",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-object-rest-spread": "^6.26.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "bootstrap": "^3.3.7",
        "chai": "^4.1.2",
        "compression-webpack-plugin": "^1.1.11",
        "cross-env": "^5.1.5",
        "esdoc": "^1.1.0",
        "esdoc-ecmascript-proposal-plugin": "^1.0.0",
        "esdoc-standard-plugin": "^1.0.0",
        "eslint": "^4.19.1",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-loader": "^2.0.0",
        "eslint-plugin-import": "^2.11.0",
        "eslint-plugin-jsx-a11y": "^6.0.3",
        "eslint-plugin-react": "^7.8.1",
        "json-loader": "^0.5.7",
        "karma": "^2.0.2",
        "karma-chai": "^0.1.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage": "^1.1.2",
        "karma-coveralls": "^1.2.1",
        "karma-mocha": "^1.3.0",
        "karma-mocha-reporter": "^2.2.5",
        "karma-webpack": "^3.0.0",
        "mocha": "^5.1.1",
        "puppeteer": "^1.4.0",
        "rimraf": "^2.6.2",
        "should": "^13.2.1",
        "sinon": "^5.0.7",
        "uglifyjs-webpack-plugin": "^1.2.5",
        "webpack": "^4.8.3",
        "webpack-cli": "^2.1.3",
        "webpack-dev-server": "^3.1.4",
        "webpack-sources": "1.1.0"
    },
    "dependencies": {
        "core-js": "^2.5.6",
        "deepmerge": "^2.1.0"
    }
